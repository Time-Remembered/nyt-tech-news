{
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jquery": true
    },
    "globals": {
      "AP": true,
      "AJS": true
    },
    "rules": {
      "react/prefer-stateless-function": 0,
      "react/prop-types": 0,
  
      "react/react-in-jsx-scope": 0,
      "react/jsx-filename-extension": 0,
  
      "import/no-named-as-default": 0,
      "import/prefer-default-export": 0,
  
      "space-infix-ops": 0,
      "arrow-body-style": 0,
      "max-len": ["warn", 120]
    }
  }
  